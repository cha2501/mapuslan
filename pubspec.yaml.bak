name: mapuslan_v2
description: A new Flutter project.
version: 1.0.0+1
environment: 
  sdk: '>=2.12.0 <3.0.0'
dependencies: 
  flutter: 
    sdk: flutter
  flutter_localizations: 
    sdk: flutter
  camera: ^0.11.0
  tflite: ^1.1.2
  cached_network_image: ^3.3.0
  http: ^1.2.1
  flutter_svg: ^2.0.9
  provider: ^6.1.1
  shared_preferences: ^2.2.2
  connectivity_plus: ^6.0.3
  geolocator: ^12.0.0
  intl: ^0.18.0
  permission_handler: ^11.3.1
  flutter_map: 6.1.0
  image_picker: ^1.1.1
  
dev_dependencies: 
  flutter_test: 
    sdk: flutter
flutter: 
  uses-material-design: true
  assets: 
    - assets/
    - assets/images/
    - assets/mapuslan_model.tflite/
    - assets/mapuslan_labels.txt/
  fonts: 
    - family: Bakbak One
      fonts: 
        - asset: assets/fonts/BakbakOneRegular.ttf
          weight: 400
    - family: Inter
      fonts: 
        - asset: assets/fonts/InterSemiBold.ttf
          weight: 600
        - asset: assets/fonts/InterRegular.ttf
          weight: 400
        - asset: assets/fonts/InterBold.ttf
          weight: 700
        - asset: assets/fonts/InterMedium.ttf
          weight: 500
    - family: Epilogue
      fonts: 
        - asset: assets/fonts/EpilogueBold.ttf
          weight: 700
        - asset: assets/fonts/EpilogueRomanExtraBold.ttf
          weight: 800
        - asset: assets/fonts/EpilogueRomanSemiBold.ttf
          weight: 600
        - asset: assets/fonts/EpilogueRomanRegular.ttf
          weight: 400
        - asset: assets/fonts/EpilogueRegular.ttf
          weight: 400
        - asset: assets/fonts/EpilogueSemiBold.ttf
          weight: 600
